---
interface Props {
	title: string;
    paragraphe: string
}

const {title, paragraphe} = Astro.props;
---

<header class="header">
    <h1>{title}</h1>
    <p>{paragraphe}</p>
</header>

<style>
.header {
  padding: 10px;
  text-align: center;
  background-color: #f1f1f1;
}
</style>